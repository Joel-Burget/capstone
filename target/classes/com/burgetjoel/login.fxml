<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="250.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.burgetjoel.LoginController">
   <children>
      <TextField fx:id="usernameField" layoutX="123.0" layoutY="54.0" />
      <PasswordField fx:id="passwordField" layoutX="123.0" layoutY="85.0" />
      <Label fx:id="usernameLabel" layoutX="56.0" layoutY="58.0" text="Username" />
      <Label fx:id="passwordLabel" layoutX="56.0" layoutY="89.0" text="Password" />
      <Button fx:id="loginButton" layoutX="123.0" layoutY="128.0" mnemonicParsing="false" onAction="#loginAction" text="Login" />
      <Button fx:id="cancelButton" layoutX="220.0" layoutY="128.0" mnemonicParsing="false" onAction="#cancelAction" text="Cancel" />
      <Label fx:id="regionLabel" layoutX="28.0" layoutY="14.0" text="Label" />
      <Label fx:id="errorLabel" layoutX="123.0" layoutY="179.0" text="Error Message" textFill="#d70f0f" visible="false" />
   </children>
</AnchorPane>
